{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "dayback",
      "host": {
        "sourcePath": "var/app/current/django_app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "dayback",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 4040
        }
      ]
    }
  ]
}